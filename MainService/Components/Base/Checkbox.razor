@{
    <BaseInput TParam="bool" Label="@Label" Type="checkbox" InputValue="field" OnChanged="@fieldChanged" Css="checkbox-component" />
}

@code {

    [Parameter]
    public string Label { get; set; }

    [Parameter]
    public bool field { get; set; }

    [Parameter]
    public EventCallback<bool> fieldChanged { get; set; }
}