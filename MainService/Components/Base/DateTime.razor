@{
    <BaseInput TParam="System.DateTime" Label="@Label" Type="datetime-local" InputValue="@InputValue" OnChanged="OnChanged" />
}

@code {

    [Parameter]
    public string Label { get; set; }

    [Parameter]
    public System.DateTime InputValue { get; set; }

    [Parameter]
    public Action<System.DateTime> OnChanged { get; set; }

}